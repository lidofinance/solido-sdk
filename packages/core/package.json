  },
  "homepage": "https://github.com/lidofinance/solido-sdk#readme",
  "dependencies": {
    "borsh": "^0.7.0",
    "bs58": "^4.0.1"
  },
  "devDependencies": {
    "@solana/buffer-layout": "4.0.0",
    "@solana/spl-token": "0.2.0",
    "@solana/wallet-adapter-base": "^0.9.9",
    "@solana/web3.js": "^1.47.3",
    "@types/bn.js": "^5.1.0",
    "bn.js": "^5.2.1"
  },
  "peerDependencies": {
    "@solana/buffer-layout": "4.0.0",
