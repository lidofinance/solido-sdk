  "description": "Lido on Solana frontend sdk",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/core/src/index.d.ts",
  "scripts": {
    "tsc": "tsc --init",
    "test": "jest",
    "build": "rollup -c ./rollup.config.mjs",
